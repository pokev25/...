! esc -> caps lock
! caps lock -> ctrl
! ctrl -> esc
remove Lock = Caps_Lock
remove Control = Control_L
keysym Escape = Caps_Lock
keysym Caps_Lock = Control_L
keysym Control_L = Escape
add Lock = Caps_Lock
add Control = Control_L

! alt-r -> hangul https://github.com/puzzlet/_/blob/57821c8895d2f790a5abb1c7041b7ba7da652b4b/.Xmodmap
keycode 108 = Hangul
remove Mod1 = Alt_R
remove Mod1 = Hangul
